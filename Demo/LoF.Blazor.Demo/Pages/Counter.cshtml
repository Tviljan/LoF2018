@page "/counter"
@using Microsoft.AspNetCore.Blazor.Browser.Interop
<h1>Counter</h1>

<p>Current count: @currentCount</p>

<button onclick="@IncrementCount">Click me</button>

<button onclick=@CallJS>Call JavaScript</button>

@functions {
    int currentCount = 0;

    void IncrementCount()
    {
        currentCount++;
    }

    private void CallJS()
    {

        RegisteredFunction.Invoke<string>(
            "BlazorLibrary.CSharp.ExampleJsInterop.Prompt", "haloo");




    }
}
